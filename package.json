{
  "name": "reactriot2018-es-community",
  "description": "Git web client",
  "homepage": "https://gallyt.netlify.com",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    "Nolan Rigo <nolan@rigo.pro>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hackbit/reactriot2018-es-community"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "precommit": "lint-staged",
    "eject": "react-scripts-ts eject"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write --parser json",
      "git add"
    ],
    "*.md": [
      "prettier --write --parser markdown",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write --parser typescript",
      "tslint --fix -p .",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts-ts": "2.17.0",
    "styled-components": "^3.4.6",
    "styled-normalize": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/styled-components": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  }
}
